		  <property>/virtual-racer/recording</property>
	     </binding>
		</key>
		<key n="82"> 
         <name>playing</name>
         <desc>virtual racer on track</desc>         
		 <binding>
		  <command>property-toggle</command>		  
		  <property>/virtual-racer/play</property>
	     </binding>
		</key>
		<key n="108"> 
         <name>Last or fastest</name>
