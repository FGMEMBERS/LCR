		<!-- Clutch with Ctrl find setlistener in gearbox.nas -->	
		<key n="263">
			<name>F7</name>
			<desc>Show map</desc>
			<binding>
			<command>dialog-show</command>
			<dialog-name>map</dialog-name>
			</binding>
		</key>
		<key n="264">
		    <name>F8</name>
		    <desc>Results</desc>
			<binding>
				<command>nasal</command>
				<script>
					var s1 = getprop("/LCR/show-race-times") or 0;
					var s2 = getprop("/road-racer/show-mp-times") or 0;
					if(s1 or s2){
						setprop("/LCR/show-race-times", 0);
						setprop("/road-racer/show-mp-times", 0);
					}else{
						setprop("/LCR/show-race-times", 1);
						setprop("/road-racer/show-mp-times", 1);
					}
				</script>
			</binding>
