	<menubar>
		<default>
		<menu n="10">
			<label>LCR-F2</label>
			<enabled type="bool">true</enabled>
			<item>
				<label>--- Tools ---</label>
				<enabled>false</enabled>
			</item>			
			<item>
				<label>Autostart / shutdown</label>
				<binding>
					<command>property-toggle</command>
					<property>sim/model/start-idling</property>
				</binding>
			</item> 		
			<enabled type="bool">true</enabled>
			<item>
				<label>Repair engine</label>
				<binding>
					<command>nasal</command>
					<script>
						var iskilled = getprop("/engines/engine/killed") or 0;
						if(iskilled > 0.2){
							settimer(func{ setprop("/engines/engine/killed",0);}, 20);
				    		screen.log.write(sprintf("Please wait 20 sec. for engine overhaul."), 1, 1, 0.0);
						}else{
				    		screen.log.write(sprintf("Everything is ok with your engine - nothing to do."), 1, 1, 0.0);
						}
					</script>
				</binding>
			</item>
			<enabled type="bool">true</enabled>				
			<item>
				<label>Show Sector/Lap Results</label>
				<binding>
					<command>property-toggle</command>
					<property>/LCR/show-race-times</property>
				</binding>
			</item>	
			<enabled type="bool">true</enabled>				
			<item>
				<label>Switch last or fastest Results</label>
				<binding>
					<command>property-toggle</command>
					<property>/LCR/switch-last-fastest</property>
				</binding>
			</item>	
 			<item>
 				<label>--- Passenger ---</label>
 				<enabled>false</enabled>
 			</item>
	        <item>
	         <label>Select Passenger</label>
	         <binding>
	          <command>nasal</command>
	          <script>
	            dual_control_tools.copilot_dialog.show(aircraft_dual_control.copilot_type);
	           </script>
	          </binding>
	         </item>
 			<item>
 				<label>--- Painting ---</label>
 				<enabled>false</enabled>
 			</item>
 			<item>
 				<label>Select Livery</label>
 				<binding>
 					<command>nasal</command>
 					<script>aircraft.livery.dialog.toggle()</script>
 				</binding>
 			</item>
			<enabled type="bool">true</enabled>
			<item>
				<label>--- Old mens mode ---</label>
				<enabled>false</enabled>
			</item>			
			<item>
				<label>Toggle greybeard view</label>
				<binding>
					<command>property-toggle</command>
					<property>/controls/LCR/old-mens-mode</property>
				</binding>
			</item>	
			<item>
				<label>Toggle throttle input axis</label>
				<binding>
					<command>property-toggle</command>
					<property>/controls/flight/select-throttle-input</property>
				</binding>
			</item> 			
			<item>
				<label>--- Roadraces ---</label>
				<enabled>false</enabled>
			</item>				
	        <item>
	          <label>Race settings</label>
	          <binding>
	            <command>nasal</command>
	            <script>lcr.config_dlg.open()</script>
	          </binding>
	        </item>
		</menu>

		</default>
	</menubar>
	
