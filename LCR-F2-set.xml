         <binding>
		  <command>property-toggle</command>
		  <property>/LCR/switch-last-fastest</property>
	     </binding>
		</key>
	  <!--shift up/down-->
        <key n="362"> 
         <name>Home</name>
         <desc>Shift Up</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
        <key n="363"> 
          <name>End</name>
          <desc>Shift Down</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">-1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
        <key n="60"> 
         <name>></name>
         <desc>Shift Up Alternative</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
        <key n="62"> 
          <name>Shift + ></name>
          <desc>Shift Down Alternative</desc>
         <binding>
          <command>property-adjust</command>
          <property>/engines/engine/gear</property>
          <step type="double">-1</step>
          <min type="double">0</min>
          <max type="double">6</max>
	   </binding>
        </key>
		<key n="8"> 
         <name>Backspace</name>
         <desc>Clutch</desc>
         <binding>
		  <command>property-assign</command>
		  <property>/engines/engine/clutch</property>
		  <value>1</value>
	     </binding>
		 <mod-up>
		  <binding>
           <command>property-assign</command>
		   <property>/engines/engine/clutch</property>
		   <value>0</value>
          </binding>
		 </mod-up>
		</key>
		<key n="83"> 
         <name>Start/Stop</name>
