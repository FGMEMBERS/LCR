<PropertyList>

  <sim>
	<description>LCR-F2 Passenger</description>
	<author>LAKE OF CONSTANCE HANGAR :: M.Kraus</author>
	<aircraft-version>0.1.5</aircraft-version>
    <flight-model>null</flight-model>

    <model>
      <path archive="y">Aircraft/LCR/Models/LCR-P.xml</path>
    </model>

    <remote>
     <pilot-callsign type="string"/>
    </remote>

    <sound>
		<path>Aircraft/LCR/Sounds/LCR-F2-sound.xml</path>
    </sound>

   	<startup>
   		<splash-texture>Aircraft/LCR/splash-LCR-F2.png</splash-texture>
   	</startup>
	
    <help>
		<key>
		  <name>F7</name>
		  <desc>Open map</desc>
		</key>
		<key>
		  <name>F8</name>
		  <desc>Open results</desc>
		</key>
		<key>
		  <name>l</name>
		  <desc>switch last and fastest lap</desc>
		</key>
    </help>
	
    <menubar>
     <default>
      <menu n="100">
      <label>LCR-F2</label>
      <enabled type="bool">true</enabled> 			
	  <item>
	    <label>--- search Team ---</label>
	    <enabled>false</enabled>
	  </item>
      <item>
       <label>Select LCR-Sidecar in range</label>
       <binding>
        <command>nasal</command>
        <script>
         dual_control_tools.copilot_dialog.show(aircraft_dual_control.pilot_type);
        </script>
       </binding>
      </item>			
	  <item>
	    <label>--- Laptimes ---</label>
	    <enabled>false</enabled>
	  </item>
		<enabled type="bool">true</enabled>				
		<item>
			<label>Show Sector/Lap Results</label>
			<binding>
				<command>property-toggle</command>
				<property>/LCR/show-race-times</property>
			</binding>
		</item>	
		<enabled type="bool">true</enabled>				
		<item>
			<label>Switch last or fastest Results</label>
			<binding>
				<command>property-toggle</command>
				<property>/LCR/switch-last-fastest</property>
			</binding>
		</item>
		<item>
			<label>--- Roadraces ---</label>
			<enabled>false</enabled>
		</item>				
        <item>
          <label>Race settings</label>
          <binding>
            <command>nasal</command>
            <script>lcr.config_dlg.open()</script>
          </binding>
        </item>	
      </menu>
     </default>
    </menubar>


    <view n="0"> 
      <name>Passenger</name>
      <internal archive="y">true</internal>
      <config>
        <limits>
          <enabled archive="y" type="bool">false</enabled>
        </limits>
        <x-offset-m archive="y">-0.61 </x-offset-m>	<!-- Right -->
        <y-offset-m archive="y"> 0.6 </y-offset-m>	<!-- Up    -->
        <z-offset-m archive="y"> 0.46 </z-offset-m><!-- Back  -->
        <default-field-of-view-deg type="double"> 70 </default-field-of-view-deg>
      </config>
    </view>
    <view n="1">
      <enabled type="bool">false</enabled>
    </view>
    <view n="2">
      <enabled type="bool">false</enabled>
    </view>
    <view n="3">
