		<key n="264">
		    <name>F8</name>
		    <desc>Results</desc>
			<binding>
				<command>nasal</command>
				<script>
					var s1 = getprop("/LCR/show-race-times") or 0;
					var s2 = getprop("/road-racer/show-mp-times") or 0;
					if(s1 or s2){
						setprop("/LCR/show-race-times", 0);
						setprop("/road-racer/show-mp-times", 0);
					}else{
						setprop("/LCR/show-race-times", 1);
						setprop("/road-racer/show-mp-times", 1);
					}
				</script>
			</binding>
		</key>
		<key n="108"> 
         <name>Last or fastest</name>
         <desc>last or fastest lap times</desc>
         <binding>
		  <command>property-toggle</command>
		  <property>/LCR/switch-last-fastest</property>
	     </binding>
		</key>
	</keyboard>
</input>

